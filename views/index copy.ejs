<!DOCTYPE html>
<html lang="en">
<head>
    <title>REV DMPT Survey</title>
    <meta charset="UTF-8">
    <link rel='stylesheet' href='/stylesheets/style.css'>
</head>
<body>
    <div class='wrapper'>
        <div class = 'main-row'>
            <div class='column light left-col'>
                <div class='row border img-row'>
                    <div class="car-preview-container">
                        <img src="/images/car-preview.png" alt="Car Preview" style="object-fit: contain;">
                    </div>
                </div>
                <div class='row border content-row'>
                    <p id="trim" class="text-padding"></p>
                </div>
            </div>
            <div class='column right-col'>
                <div class='row light content-row'>
                    <div class='column client-col'>
                        <div class='row title-row border'>
                            <p class="text-padding title">Client</p>
                        </div>
                        <div class='row title-row border'>
                            <p id="client" class="text-padding entry">General Motors</p>
                        </div>
                    </div>
                    <div class='column participant-col'>
                        <div class='row title-row border'>
                            <p class="text-padding title">Participant ID</p>
                        </div>
                        <div class='row title-row border'>
                            <p id="survey_id" class="text-padding entry">01246</p>
                        </div>
                    </div>
                </div>
                <div class='row title-row dark border'>
                    <p class="text-padding title">Data Parameters: Cadillac Lyriq</p>
                </div>
                <div class='row dark'>
                    <div class='column'>
                        <div class='row title-row'>
                            <div class='column border field-col'>
                            </div>
                            <div class='column border field-col'>
                                <p class="text-padding title">Trim</p>
                            </div>
                            <div class='column border field-col'>
                                <p class="text-padding title">Wheels</p>
                            </div>
                            <div class='column border field-col'>
                                <p class="text-padding title">Exterior</p>
                            </div>
                            <div class='column border field-col'>
                                <p class="text-padding title">Interior</p>
                            </div>
                            <div class='column border field-col'>
                                <p class="text-padding title">Total</p>
                            </div>
                        </div>
                        <div class='row field-row'>
                            <div class='column border field-col'>
                                <p class="text-padding title">Config</p>
                            </div>
                            <div class='column border field-col'>
                                <p id="trim" class="text-padding entry"></p>
                            </div>
                            <div class='column border field-col'>
                                <p id="wheels" class="text-padding entry"></p>
                            </div>
                            <div class='column border field-col'>
                                <p id="exterior" class="text-padding entry"></p>
                            </div>
                            <div class='column border field-col'>
                                <p id="interior" class="text-padding entry"></p>
                            </div>
                            <div class='column border field-col'>
                            </div>
                        </div>
                        <div class='row field-row'>
                            <div class='column border field-col'>
                                <p class="text-padding title">Config Time</p>
                            </div>
                            <div class='column border field-col'>
                                <p id="trimTime" class="text-padding entry"></p>
                            </div>
                            <div class='column border field-col'>
                                <p id="wheelsTime" class="text-padding entry"></p>
                            </div>
                            <div class='column border field-col'>
                                <p id="exteriorTime" class="text-padding entry"></p>
                            </div>
                            <div class='column border field-col'>
                                <p id="interiorTime" class="text-padding entry"></p>
                            </div>
                            <div class='column border field-col'>
                                <p id="totalTime" class="text-padding entry"></p>
                            </div>
                        </div>
                        <div class='row field-row'>
                            <div class='column border field-col'>
                                <p class="text-padding title">Percent Config.</p>
                            </div>
                            <div class='column border field-col'>
                                <p id="trimPercent" class="text-padding entry"></p>
                            </div>
                            <div class='column border field-col'>
                                <p id="wheelsPercent" class="text-padding entry"></p>
                            </div>
                            <div class='column border field-col'>
                                <p id="exteriorPercent" class="text-padding entry"></p>
                            </div>
                            <div class='column border field-col'>
                                <p id="interiorPercent" class="text-padding entry"></p>
                            </div>
                            <div class='column border field-col'>
                            </div>
                        </div>
                    </div>
                </div>
                <div class='row light title-row'>
                    <div class='column border field-col'>
                        <p class="text-padding"></p> 
                    </div>
                    <div class='column border field-col'>
                        <p class="text-padding title">Total</p>
                    </div>
                    <div class='column border field-col'></div>
                    <div class='column border field-col'></div>
                    <div class='column border field-col'></div>
                    <div class='column border field-col'></div>
                </div>
                <div class='row dark field-row'>
                    <div class='column border field-col'>
                        <p class="text-padding title">Time in Survey</p>
                    </div>
                    <div class='column border field-col'>
                        <p id="timeInSurvey" class="text-padding entry"></p>
                    </div>
                    <div class='column border field-col'></div>
                    <div class='column border field-col'></div>
                    <div class='column border field-col'></div>
                    <div class='column border field-col'></div>
                </div>
                <div class='row light title-row'>
                    <div class='column border field-col'>
                        <p class="text-padding title">Survey</p>
                    </div>
                    <div class='column border field-col'>
                        <p class="text-padding title">Results</p>
                    </div>
                    <div class='column border survey-col'>
                        <p class="text-padding title">Total Score</p>
                    </div>
                </div>
                <div class='row dark field-row'>
                    <div class='column field-col'>
                        <div class='row border'>
                            <p class="text-padding title">Question 1</p>
                        </div>
                        <div class='row border'>
                            <p class="text-padding title">Question 2</p>
                        </div>
                        <div class='row border'>
                            <p class="text-padding title">Question 3</p>
                        </div>
                        <div class='row border'>
                            <p class="text-padding title">Question 4</p>
                        </div>
                        <div class='row border'>
                            <p class="text-padding title">Question 5</p>
                        </div>
                    </div>
                    <div class='column field-col'>
                        <div class='row border'>
                            <p id="question1" class="text-padding entry"></p>
                        </div>
                        <div class='row border'>
                            <p id="question2" class="text-padding entry"></p>
                        </div>
                        <div class='row border'>
                            <p id="question3" class="text-padding entry"></p>
                        </div>
                        <div class='row border'>
                            <p id="question4" class="text-padding entry"></p>
                        </div>
                        <div class='row border'>
                            <p id="question5" class="text-padding entry"></p>
                        </div>
                    </div>
                    <div class='column border survey-col'>
                            <p id="surveyTotal" class="text-padding entry"></p>
                        <p id="surveyRating" class="text-padding entry"></p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script>
        // Pass server-side variables to client-side
        var serverData = {
            trim: "<%= typeof trim !== 'undefined' ? trim : '' %>",
            wheels: "<%= typeof wheels !== 'undefined' ? wheels : '' %>",
            exterior: "<%= typeof exterior !== 'undefined' ? exterior : '' %>",
            interior: "<%= typeof interior !== 'undefined' ? interior : '' %>",
            trimTime: "<%= typeof trimTime !== 'undefined' ? trimTime : '' %>",
            wheelsTime: "<%= typeof wheelsTime !== 'undefined' ? wheelsTime : '' %>",
            exteriorTime: "<%= typeof exteriorTime !== 'undefined' ? exteriorTime : '' %>",
            interiorTime: "<%= typeof interiorTime !== 'undefined' ? interiorTime : '' %>",
            totalTime: "<%= typeof totalTime !== 'undefined' ? totalTime : '' %>",
            trimPercent: "<%= typeof trimPercent !== 'undefined' ? trimPercent : '' %>",
            wheelsPercent: "<%= typeof wheelsPercent !== 'undefined' ? wheelsPercent : '' %>",
            exteriorPercent: "<%= typeof exteriorPercent !== 'undefined' ? exteriorPercent : '' %>",
            interiorPercent: "<%= typeof interiorPercent !== 'undefined' ? interiorPercent : '' %>",
            timeInSurvey: "<%= typeof timeInSurvey !== 'undefined' ? timeInSurvey : '' %>",
            question1: "<%= typeof question1 !== 'undefined' ? question1 : '' %>",
            question2: "<%= typeof question2 !== 'undefined' ? question2 : '' %>",
            question3: "<%= typeof question3 !== 'undefined' ? question3 : '' %>",
            question4: "<%= typeof question4 !== 'undefined' ? question4 : '' %>",
            question5: "<%= typeof question5 !== 'undefined' ? question5 : '' %>",
            surveyTotal: "<%= typeof surveyTotal !== 'undefined' ? surveyTotal : '' %>",
            surveyRating: "<%= typeof surveyRating !== 'undefined' ? surveyRating : '' %>"
        };
    </script>
    <script src="/javascripts/index.js"></script>
</body>
</html>