<!DOCTYPE html>
<html lang="en">
<head>
    <title>REV DMPT Survey</title>
    <meta charset="UTF-8">
    <link rel='stylesheet' href='/stylesheets/style.css'>
</head>
<body>
    <div class='wrapper'>
        <div class="dark car-preview-container">
            <img src="/images/gm-logo-1.png" alt="Car Preview" style="object-fit: contain;">
        </div>
        <div class="light client">
            <p class="title">Client</p>
        </div>
        <div class="light participant-id">
            <p class="title"> Participant ID</p>
        </div>
        <div class="dark client">
            <p class="client-entry">General Motors</p>
        </div>
        <div class="dark participant-id">
            <p class="entry">01246</p>
        </div>
        <div class="dark left-col"></div>
        <div class="dark field-row">
            <p class="title">Data Parameters: Cadillac Lyriq</p>
        </div>
        <div class="light"></div>
        <div class="light">
            <p class="title">Trim</p>
        </div>
        <div class="light">
            <p class="title">Wheels</p>
        </div>
        <div class="light">
            <p class="title">Exterior</p>
        </div>
        <div class="light">
            <p class="title">Interior</p>
        </div>
        <div class="light">
            <p class="title">Total</p>
        </div>
        <div class="dark">
            <p class="title">Config</p>
        </div>
        <div class="dark">
            <p id="trim" class="entry"></p>
        </div>
        <div class="dark">
            <p id="wheels" class="entry"></p>
        </div>
        <div class="dark">
            <p id="exterior" class="entry"></p>
        </div>
        <div class="dark">
            <p id="interior" class="entry"></p>
        </div>
        <div class="dark"></div>
        <div class="dark">
            <p class="title">Config Time</p>
        </div>
        <div class="dark">
            <p id="trimTime" class="entry"></p>
        </div>
        <div class="dark">
            <p id="wheelsTime" class="entry"></p>
        </div>
        <div class="dark">
            <p id="exteriorTime" class="entry"></p>
        </div>
        <div class="dark">
            <p id="interiorTime" class="entry"></p>
        </div>
        <div class="dark">
            <p id="totalTime" class="entry"></p>
        </div>
        <div class="dark">
            <p class="title">Percent Config</p>
        </div>
        <div class="dark">
            <p id="trimPercent" class="entry"></p>
        </div>
        <div class="dark">
            <p id="wheelsPercent" class="entry"></p>
        </div>
        <div class="dark">
            <p id="exteriorPercent" class="entry"></p>
        </div>
        <div class="dark">
            <p id="interiorPercent" class="entry"></p>
        </div>
        <div class="dark"></div>
        <div class="light"></div>
        <div class="light">
            <p class="title">Total</p>
        </div>
        <div class="light"></div>
        <div class="light"></div>
        <div class="light"></div>
        <div class="light"></div>
        <div class="dark">
            <p class="title">Time in Survey</p>
        </div>
        <div class="dark">
            <p id="timeInSurvey" class="entry"></p>
        </div>
        <div class="dark"></div>
        <div class="dark"></div>
        <div class="dark"></div>
        <div class="dark"></div>
        <div class="light">
            <p class="title">Survey</p>
        </div>
        <div class="light">
            <p class="title">Results</p>
        </div>
        <div class="light survey-col">
            <p class="title">Total Score</p>
        </div>
        <div class="dark">
            <p class="title">Question 1</p>
        </div>
        <div class="dark">
            <p id="question1" class="entry"></p>
        </div>
        <div class="survey-results-col dark">
            <p id="surveyTotal" class="total-points"></p>
            <p id="surveyRating" class="total-rating"></p>
        </div>
        <div class="dark">
            <p class="title">Question 2</p>
        </div>
        <div class="dark">
            <p id="question2" class="entry"></p>
        </div>
        <div class="dark">
            <p class="title">Question 3</p>
        </div>
        <div class="dark">
            <p id="question3" class="entry"></p>
        </div>
        <div class="dark">
            <p class="title">Question 4</p>
        </div>
        <div class="dark">
            <p id="question4" class="entry"></p>
        </div>
        <div class="dark">
            <p class="title">Question 5</p>
        </div>
        <div class="dark">
            <p id="question5" class="entry"></p>
        </div>
    </div>
    <script>
        // Pass server-side variables to client-side
        var serverData = {
            trim: "<%= typeof trim !== 'undefined' ? trim : '' %>",
            wheels: "<%= typeof wheels !== 'undefined' ? wheels : '' %>",
            exterior: "<%= typeof exterior !== 'undefined' ? exterior : '' %>",
            interior: "<%= typeof interior !== 'undefined' ? interior : '' %>",
            trimTime: "<%= typeof trimTime !== 'undefined' ? trimTime : '' %>",
            wheelsTime: "<%= typeof wheelsTime !== 'undefined' ? wheelsTime : '' %>",
            exteriorTime: "<%= typeof exteriorTime !== 'undefined' ? exteriorTime : '' %>",
            interiorTime: "<%= typeof interiorTime !== 'undefined' ? interiorTime : '' %>",
            totalTime: "<%= typeof totalTime !== 'undefined' ? totalTime : '' %>",
            trimPercent: "<%= typeof trimPercent !== 'undefined' ? trimPercent : '' %>",
            wheelsPercent: "<%= typeof wheelsPercent !== 'undefined' ? wheelsPercent : '' %>",
            exteriorPercent: "<%= typeof exteriorPercent !== 'undefined' ? exteriorPercent : '' %>",
            interiorPercent: "<%= typeof interiorPercent !== 'undefined' ? interiorPercent : '' %>",
            timeInSurvey: "<%= typeof timeInSurvey !== 'undefined' ? timeInSurvey : '' %>",
            question1: "<%= typeof question1 !== 'undefined' ? question1 : '' %>",
            question2: "<%= typeof question2 !== 'undefined' ? question2 : '' %>",
            question3: "<%= typeof question3 !== 'undefined' ? question3 : '' %>",
            question4: "<%= typeof question4 !== 'undefined' ? question4 : '' %>",
            question5: "<%= typeof question5 !== 'undefined' ? question5 : '' %>",
            surveyTotal: "<%= typeof surveyTotal !== 'undefined' ? surveyTotal : '' %>",
            surveyRating: "<%= typeof surveyRating !== 'undefined' ? surveyRating : '' %>"
        };
    </script>
    <script src="/javascripts/index.js"></script>
</body>
</html>